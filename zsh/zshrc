# Options 
. ~/.zshoptions

# Parameters 
. ~/.zshparam
. ~/.zshprompt

# Aliases 
. ~/.zshalias

# Zle 
. ~/.zshzle

# Completion 
. ~/.zshcompsys

# Local 

[[ -f ~/.zshrc_local ]] && . ~/.zshrc_local

# Automatically generated by initializor

[[ -f ~/.zshbookmarks ]] && . ~/.zshbookmarks
[[ -f ~/.zshpaths ]] && . ~/.zshpaths

# show directory content on cd
function chpwd() {
	emulate -LR zsh
	l
}

# artificial intelligence
trap "print; print '$fg[red]'ouch; return" INT

eval `keychain --eval id_dsa`
